{- This is a
   {- nested -} multiline comment with "strings" -}

-- This is a single line comment

foo :: Int -> Int -> Int;
foo a b = a + b * c;
foo a b = a * b + c;
foo a b = ~a * b;
foo f b = f b c;

tuple :: (Int -> String) -> Int -> (Int, String);
tuple f a = (a, f a);

list :: Int -> [(Int, ())];
list a = (a, ()) : [];

